{
	"top-table-list": {
		"styles": {
			"default": {
				"grid-template-columns": "1fr",
				"grid-template-rows": "auto",
				"gap": "var(--global-spacing-5)",
				"components": {
					"grid": {
						"gap": "var(--global-spacing-5)"
					}
				}
			},
			"desktop": {}
		}
	},
	"top-table-list-large": {
		"styles": {
			"default": {
				"gap": "var(--global-spacing-4)",
				"components": {
					"link": {
						"position": "relative"
					}
				}
			},
			"desktop": {
				"gap": "var(--global-spacing-6)",
				"components": {
					"divider": {
						"grid-column": "span 2"
					},
					"grid": {
						"gap": "var(--global-spacing-5)"
					}
				}
			}
		}
	},
	"top-table-list-large-container": {
		"styles": {
			"default": {
				"grid-template-columns": "1fr"
			},
			"desktop": {}
		}
	},
	"top-table-list-large-icon-label": {
		"styles": {
			"default": {
				"align-items": "center",
				"padding": "var(--global-spacing-2)",
				"background-color": "var(--color-primary)",
				"border": 0,
				"border-radius": "var(--global-spacing-2)",
				"bottom": "var(--global-spacing-2)",
				"margin-left": "var(--global-spacing-2)",
				"position": "absolute",
				"z-index": 1,
				"components": {
					"icon": {
						"fill": "var(--global-white)",
						"height": "var(--global-spacing-4)",
						"width": "var(--global-spacing-4)"
					}
				}
			},
			"desktop": {}
		}
	},
	"top-table-list-large-label": {
		"styles": {
			"default": {
				"margin-left": "var(--global-spacing-2)",
				"color": "var(--global-white)"
			},
			"desktop": {}
		}
	},
	"top-table-list-large-text": {
		"styles": {
			"default": {
				"gap": 0,
				"components": {
					"heading": {
						"font-size": "var(--heading-level-3-font-size)",
						"line-height": "var(--heading-level-3-line-height)"
					},
					"overline": {
						"font-size": "var(--heading-level-6-font-size)",
						"line-height": "var(--heading-level-6-line-height)"
					},
					"paragraph": {
						"font-size": "var(--body-font-size)",
						"line-height": "var(--body-line-height)"
					},
					"stack": {
						"gap": "var(--global-spacing-4)"
					},
					"separator": {
						"margin": "0var(--global-spacing-2) 0 var(--global-spacing-2)"
					}
				}
			},
			"desktop": {
				"components": {
					"heading": {
						"font-size": "var(--heading-level-4-font-size)",
						"line-height": "var(--heading-level-4-line-height)"
					}
				}
			}
		}
	},
	"top-table-list-medium": {
		"styles": {
			"default": {
				"components": {
					"attribution": {
						"display": "block",
						"margin-top": "var(--global-spacing-4)"
					},
					"divider": {
						"clear": "both",
						"margin-top": "var(--global-spacing-5)"
					},
					"media-item": {
						"float": "right",
						"margin": "00 var(--global-spacing-5) 0",
						"max-width": "100px"
					},
					"heading": {
						"font-size": "var(--heading-level-5-font-size)",
						"font-weight": "var(--heading-level-5-font-weight)",
						"line-height": "var(--heading-level-5-line-height)",
						"margin-bottom": "var(--global-spacing-4)"
					},
					"paragraph": {
						"display": "block"
					},
					"separator": {
						"padding": "0var(--global-spacing-2)"
					},
					"link": {
						"position": "relative"
					}
				}
			},
			"desktop": {
				"components": {
					"attribution": {
						"display": "block"
					},
					"media-item": {
						"float": "left",
						"max-width": "33%"
					},
					"paragraph": {
						"display": "block"
					}
				}
			}
		}
	},
	"top-table-list-medium-container": {
		"styles": {
			"default": {
				"grid-template-columns": "1fr",
				"grid-template-rows": "auto",
				"gap": "var(--global-spacing-8)"
			},
			"desktop": {
				"gap": "var(--global-spacing-5)"
			}
		}
	},
	"top-table-list-medium-icon-label": {
		"styles": {
			"default": {
				"align-items": "center",
				"padding": "var(--global-spacing-2)",
				"background-color": "var(--color-primary)",
				"border": 0,
				"border-radius": "var(--global-spacing-2)",
				"bottom": "var(--global-spacing-2)",
				"margin-left": "var(--global-spacing-2)",
				"position": "absolute",
				"z-index": 1,
				"components": {
					"icon": {
						"fill": "var(--global-white)",
						"height": "var(--global-spacing-4)",
						"width": "var(--global-spacing-4)"
					}
				}
			},
			"desktop": {}
		}
	},
	"top-table-list-medium-label": {
		"styles": {
			"default": {
				"margin-left": "var(--global-spacing-2)",
				"color": "var(--global-white)"
			},
			"desktop": {}
		}
	},
	"top-table-list-medium-show-image": {
		"styles": {
			"default": {
				"components": {
					"heading": {
						"width": "68%"
					}
				}
			},
			"desktop": {
				"components": {
					"attribution": {
						"margin-left": "calc(33% + var(--global-spacing-6))"
					},
					"heading": {
						"margin-left": "calc(33% + var(--global-spacing-6))",
						"width": "auto"
					},
					"paragraph": {
						"margin-left": "calc(33% + var(--global-spacing-6))"
					}
				}
			}
		}
	},
	"top-table-list-small": {
		"styles": {
			"default": {
				"display": "grid",
				"gap": "var(--global-spacing-5) var(--global-spacing-6)",
				"grid-template-columns": "1fr",
				"components": {
					"heading": {
						"font-size": "var(--body-font-size)",
						"line-height": "var(--body-line-height)"
					}
				}
			},
			"desktop": {}
		}
	},
	"top-table-list-small-above": {
		"styles": {
			"default": {
				"components": {
					"divider": {
						"grid-column": "span 1"
					}
				}
			},
			"desktop": {}
		}
	},
	"top-table-list-small-below": {
		"styles": {
			"default": {
				"components": {
					"divider": {
						"grid-column": "span 1"
					}
				}
			},
			"desktop": {}
		}
	},
	"top-table-list-small-container-1": {
		"styles": {
			"default": {},
			"desktop": {
				"components": {
					"grid": {
						"template-columns": "1fr"
					}
				}
			}
		}
	},
	"top-table-list-small-container-2": {
		"styles": {
			"default": {},
			"desktop": {
				"components": {
					"grid": {
						"template-columns": "repeat(2, minmax(5rem, 1fr))"
					}
				}
			}
		}
	},
	"top-table-list-small-container-3": {
		"styles": {
			"default": {},
			"desktop": {
				"components": {
					"grid": {
						"template-columns": "repeat(3, minmax(5rem, 1fr))"
					}
				}
			}
		}
	},
	"top-table-list-small-container-4": {
		"styles": {
			"default": {},
			"desktop": {
				"components": {
					"grid": {
						"template-columns": "repeat(4, minmax(5rem, 1fr))"
					}
				}
			}
		}
	},
	"top-table-list-small-left": {
		"styles": {
			"default": {
				"grid-template-columns": "1fr 2fr",
				"components": {
					"divider": {
						"grid-column": "span 2"
					}
				}
			},
			"desktop": {}
		}
	},
	"top-table-list-small-right": {
		"styles": {
			"default": {
				"grid-template-columns": "2fr 1fr",
				"components": {
					"divider": {
						"grid-column": "span 2"
					}
				}
			},
			"desktop": {}
		}
	},
	"top-table-list-xl": {
		"styles": {
			"default": {
				"components": {
					"heading": {
						"align-self": "center",
						"justify-content": "center"
					},
					"overline": {
						"display": "flex",
						"justify-content": "center"
					},
					"separator": {
						"padding": "0var(--global-spacing-2)"
					},
					"stack": {
						"gap": "var(--global-spacing-4)"
					}
				}
			},
			"desktop": {
				"components": {
					"heading": {
						"font-size": "var(--heading-level-1-font-size)",
						"justify-content": "center",
						"line-height": "var(--heading-level-1-line-height)"
					},
					"stack": {
						"gap": "var(--global-spacing-5)"
					}
				}
			}
		}
	},
	"top-table-list-xl-container": {
		"styles": {
			"default": {
				"grid-template-columns": "1fr"
			},
			"desktop": {}
		}
	}
}

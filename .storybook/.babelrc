{
	"presets": [
		"@babel/preset-env",
		[
			"@babel/preset-react",
			{
				"runtime": "automatic"
			}
		]
	],
	"plugins": [
		["@babel/plugin-proposal-private-property-in-object", { "loose": true }],
		["@babel/plugin-proposal-decorators", { "legacy": true }],
		"@babel/plugin-proposal-nullish-coalescing-operator"
	]
}

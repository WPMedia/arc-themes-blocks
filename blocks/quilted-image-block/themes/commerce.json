{
	"quilted-image": {
		"styles": {
			"default": {
				"margin-inline-start": "auto",
				"margin-inline-end": "auto",
				"inline-size": "100%",
				"components": {
					"heading": {
						"font-size": "var(--heading-level-5-font-size)",
						"font-weight": "var(--heading-level-5-font-weight)",
						"line-height": "var(--heading-level-5-line-height)",
						"margin-inline-start": "auto",
						"margin-inline-end": "auto",
						"margin-block-end": "var(--global-spacing-2)",
						"max-inline-size": "var(--content-scale-width)",
						"inline-size": "100%"
					}
				}
			},
			"desktop": {
				"max-inline-size": "var(--content-scale-width)",
				"inline-size": "100%",
				"components": {
					"heading": {
						"font-size": "var(--heading-level-5-font-size)",
						"line-height": "var(--heading-level-5-line-height)"
					}
				}
			}
		}
	},
	"quilted-image__media-panel": {
		"styles": {
			"default": {
				"text-decoration": "none"
			},
			"desktop": {}
		}
	},
	"quilted-image__overlay": {
		"styles": {
			"default": {
				"align-self": "end",
				"margin-block-end": "var(--global-spacing-4)",
				"margin-inline-start": "auto",
				"margin-inline-end": "auto",
				"max-inline-size": "var(--content-scale-width)",
				"inline-size": "100%",
				"components": {
					"paragraph": {
						"font-weight": "var(--heading-level-5-font-weight)",
						"line-height": "var(--heading-level-5-line-height)",
						"font-size": "var(--heading-level-5-font-size)"
					},
					"stack": {
						"gap": "var(--global-spacing-4)",
						"align-items": "start"
					}
				}
			},
			"desktop": {
				"margin-inline-start": "var(--global-spacing-6)",
				"margin-block-end": "var(--global-spacing-6)",
				"max-inline-size": "100%",
				"components": {
					"paragraph": {
						"font-size": "var(--heading-level-5-font-size)"
					},
					"stack": {
						"gap": "var(--global-spacing-4)"
					}
				}
			}
		}
	},
	"quilted-image__overlay-text--light": {
		"styles": {
			"default": {
				"color": "var(--global-white)"
			},
			"desktop": {}
		}
	},
	"quilted-image__overlay-text--dark": {
		"styles": {
			"default": {
				"color": "var(--global-black)"
			},
			"desktop": {}
		}
	},
	"quilted-image__wrapper": {
		"styles": {
			"default": {
				"gap": "var(--global-spacing-6)",
				"grid-template-areas": "'top' 'middle' 'bottom'",
				"grid-auto-flow": "column",
				"margin-block-start": "0auto",
				"margin-inline-end": "0auto",
				"margin-block-end": "0auto",
				"margin-inline-start": "0auto",
				"inline-size": "100%",
				"max-inline-size": "100%"
			},
			"desktop": {
				"grid-template-areas": "'top top' 'bottom bottom'",
				"max-inline-size": "var(--content-scale-width)",
				"inline-size": "100%"
			}
		}
	}
}

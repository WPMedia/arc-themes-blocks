{
  "packages": [
    "blocks/*"
  ],
  "version": "independent",
  "command": {
    "publish": {
      "ignoreChanges": [
        "package.json",
        "CHANGELOG.md",
        "package-lock.json"
      ],
      "conventionalCommits": true,
      "message": "chore(release): publish",
      "registry": "https://npm.pkg.github.com",
      "allowBranch": [
        "stable",
        "beta",
        "rc",
        "canary"
      ]
    }
  }
}

{
	"article-body": {
		"styles": {
			"default": {
				"font-family": "var(--font-family-primary)",
				"font-size": "var(--body-font-size)",
				"components": {
					"link": {
						"color": "var(--color-primary)"
					},
					"paragraph": {
						"display": "block",
						"overflow": "initial"
					},
					"media-item-fig-caption": {
						"background-color": "var(--global-white)",
						"line-height": "var(--body-line-height-5)"
					},
					"icon": {
						"fill": "currentColor"
					}
				}
			},
			"desktop": {
				"font-size": "var(--heading-level-6-font-size)"
			}
		}
	},
	"article-body-blockquote": {
		"styles": {
			"default": {
				"box-shadow": "inset 5px 0 0 0 var(--global-neutral-4)",
				"font-style": "italic",
				"gap": "var(--global-spacing-2)",
				"padding": "var(--global-spacing-4) 0 var(--global-spacing-4) var(--global-spacing-5)",
				"text-align": "left",
				"components": {
					"paragraph": {
						"font-size": "var(--heading-level-6-font-size)",
						"line-height": "var(--heading-level-6-line-height)"
					}
				}
			},
			"desktop": {}
		}
	},
	"article-body-children": {
		"styles": {
			"default": {
				"margin-bottom": "var(--global-spacing-5)"
			},
			"desktop": {}
		}
	},
	"article-body-citation": {
		"styles": {
			"default": {
				"font-family": "var(--font-family-primary)",
				"font-style": "normal",
				"font-size": "var(--body-font-size)"
			},
			"desktop": {}
		}
	},
	"article-body-correction": {
		"styles": {
			"default": {
				"box-shadow": "inset 0 -1px 0 0 var(--border-color)",
				"font-style": "italic",
				"padding-bottom": "var(--global-spacing-6)",
				"components": {
					"heading": {
						"font-size": "var(--heading-level-6-font-size)",
						"text-transform": "uppercase"
					},
					"paragraph": {
						"color": "var(--global-neutral-6)"
					}
				}
			},
			"desktop": {}
		}
	},
	"article-body-divider": {
		"styles": {
			"default": {
				"background-color": "var(--border-color)",
				"height": "1px"
			},
			"desktop": {}
		}
	},
	"article-body-embed-responsive": {
		"styles": {
			"default": {
				"aspect-ratio": "calc(16 / 9)",
				"width": "100%"
			},
			"desktop": {}
		}
	},
	"article-body-embed-responsive-item": {
		"styles": {
			"default": {
				"aspect-ratio": "calc(16 / 9)",
				"height": "auto",
				"width": "100%"
			},
			"desktop": {}
		}
	},
	"article-body-gallery-fullscreen": {
		"styles": {
			"default": {
				"components": {
					"media-item-fig-caption": {
						"padding": "0 var(--global-spacing-5)"
					}
				}
			},
			"desktop": {}
		}
	},
	"article-body-gallery-track-button": {
		"styles": {
			"default": {
				"color": "var(--global-white)",
				"components": {
					"button-hover": {
						"color": "var(--global-neutral-3)"
					},
					"icon": {
						"fill": "currentColor",
						"height": "var(--global-spacing-8)",
						"width": "var(--global-spacing-8)"
					}
				}
			},
			"desktop": {}
		}
	},
	"article-body-h2": {
		"styles": {
			"default": {
				"font-size": "var(--heading-level-2-font-size)",
				"line-height": "var(--heading-level-2-line-height)",
				"font-family": "var(--font-family-primary)",
				"font-weight": "bold"
			},
			"desktop": {}
		}
	},
	"article-body-h3": {
		"styles": {
			"default": {
				"font-size": "var(--heading-level-3-font-size)",
				"line-height": "var(--heading-level-3-line-height)",
				"font-family": "var(--font-family-primary)",
				"font-weight": "bold"
			},
			"desktop": {}
		}
	},
	"article-body-h4": {
		"styles": {
			"default": {
				"font-size": "var(--heading-level-4-font-size)",
				"line-height": "var(--heading-level-4-line-height)",
				"font-family": "var(--font-family-primary)",
				"font-weight": "bold"
			},
			"desktop": {}
		}
	},
	"article-body-h5": {
		"styles": {
			"default": {
				"font-size": "var(--heading-level-5-font-size)",
				"line-height": "var(--heading-level-5-line-height)",
				"font-family": "var(--font-family-primary)",
				"font-weight": "bold"
			},
			"desktop": {}
		}
	},
	"article-body-h6": {
		"styles": {
			"default": {
				"font-size": "var(--heading-level-6-font-size)",
				"line-height": "var(--heading-level-6-line-height)",
				"font-family": "var(--font-family-primary)",
				"font-weight": "bold"
			},
			"desktop": {}
		}
	},
	"article-body-image-float-left": {
		"styles": {
			"default": {
				"width": "50%",
				"float": "left",
				"margin-right": "var(--global-spacing-5)"
			},
			"desktop": {}
		}
	},
	"article-body-image-float-right": {
		"styles": {
			"default": {
				"width": "50%",
				"float": "right",
				"margin-left": "var(--global-spacing-5)"
			},
			"desktop": {}
		}
	},
	"article-body-image-wrapper": {
		"styles": {
			"default": {
				"align-items": "center",
				"background-color": "var(--global-black)",
				"display": "flex",
				"justify-content": "center",
				"max-height": "75vh",
				"components": {
					"image": {
						"height": "auto",
						"max-height": "75vh",
						"max-width": "100%"
					}
				}
			},
			"desktop": {}
		}
	},
	"article-body-interstitial-link": {
		"styles": {
			"default": {
				"font-family": "var(--font-family-primary)",
				"font-style": "italic",
				"components": {
					"link": {
						"border-bottom": "1px solid var(--color-primary)",
						"color": "var(--color-primary)"
					}
				}
			},
			"desktop": {}
		}
	},
	"article-body-link": {
		"styles": {
			"default": {
				"color": "var(--color-primary)"
			},
			"desktop": {}
		}
	},
	"article-body-ol": {
		"styles": {
			"default": {
				"font-size": "var(--body-font-size)",
				"line-height": "var(--body-line-height)",
				"list-style-position": "inside",
				"list-style-type": "decimal"
			},
			"desktop": {}
		}
	},
	"article-body-pullquote": {
		"styles": {
			"default": {
				"box-shadow": "(inset 0 -5px 0 0 var(--border-color), inset 0 5px 0 0 var(--border-color))",
				"gap": "var(--global-spacing-4)",
				"padding": "var(--global-spacing-6) 0 var(--global-spacing-5)",
				"text-align": "center",
				"components": {
					"paragraph": {
						"font-style": "italic",
						"font-size": "var(--heading-level-5-font-size)",
						"font-weight": "var(--global-font-weight-7)",
						"line-height": "var(--heading-level-5-line-height)"
					}
				}
			},
			"desktop": {}
		}
	},
	"article-body-table": {
		"styles": {
			"default": {
				"font-family": "var(--font-family-primary)",
				"font-size": "var(--body-font-size-small)",
				"line-height": "var(--body-line-height-small)",
				"text-align": "center",
				"width": "100%"
			},
			"desktop": {}
		}
	},
	"article-body-table-td": {
		"styles": {
			"default": {
				"border": "1px solid var(--border-color)",
				"padding": "var(--global-spacing-1) var(--global-spacing-2)"
			},
			"desktop": {}
		}
	},
	"article-body-table-th": {
		"styles": {
			"default": {
				"border": "1px solid var(--border-color)",
				"font-weight": "var(--global-font-weight-7)",
				"padding": "var(--global-spacing-1) var(--global-spacing-2)"
			},
			"desktop": {}
		}
	},
	"article-body-table-thead": {
		"styles": {
			"default": {
				"background-color": "var(--global-neutral-4)",
				"border": "2px solid var(--border-color)"
			},
			"desktop": {}
		}
	},
	"article-body-ul": {
		"styles": {
			"default": {
				"font-size": "var(--body-font-size)",
				"line-height": "var(--body-line-height)",
				"list-style-position": "inside",
				"list-style-type": "disc"
			},
			"desktop": {}
		}
	}
}

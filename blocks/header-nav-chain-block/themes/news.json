			"default": {
				"align-items": "center",
				"display": "flex",
				"justify-content": "space-between",
				"width": "100%",
				"gap": "var(--global-spacing-5)"
			},
			"desktop": {}
		}
	},
	"header-nav-chain-top-layout-last-child": {
		"styles": {
			"default": {
				"margin-right": 0,
				"flex-shrink": 0

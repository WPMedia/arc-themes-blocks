{
	"packages": ["blocks/*"],
	"ignoreChanges": ["**/node_modules/**", "**/package-lock.json"],
	"version": "independent",
	"command": {
		"publish": {
			"ignoreChanges": ["package.json", "CHANGELOG.md", "package-lock.json"],
			"conventionalCommits": true,
			"message": "chore(release): publish",
			"registry": "https://npm.pkg.github.com"
		}
	}
}

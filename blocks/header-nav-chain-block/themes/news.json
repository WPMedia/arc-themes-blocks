					"link": {
						"width": "100%",
						"padding": "var(--global-spacing-2) var(--global-spacing-5)",
						"align-items": "center",
						"color": "var(--global-white)",
						"font-weight": "var(--global-font-weight-7)",
						"display": "block",
						"font-size": "var(--body-font-size)",
						"justify-content": "space-between",

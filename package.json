    "lint:styles:fix": "stylelint '**/*.scss' --fix",
    "lint:fix": "npm run lint -- --fix",
    "storybook": "start-storybook -p 60001",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "latest-stable-parity": "lerna exec --no-bail --parallel -- npm dist-tag add \\${LERNA_PACKAGE_NAME}@$(node -e \"console.log(require('./lerna.json').version)\") stable",
    "build-storybook": "build-storybook -c .storybook -o news-theme-blocks",
    "test:coverage": "jest --coverage",
    "generate:intl": "node locale/scripts/generate-intl.js",
    "postinstall": "npx lerna clean -y"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:coverage && npm run lint"
    }
  },
